#include <dt-bindings/camera/exynos_is_dt.h>


#include "module_2la.dtsi" /* Ultra Wide CAMERA */
#include "module_3lu.dtsi" /* FRONT CAMERA */
#include "module_hm6_19p2.dtsi" /* REAR CAMERA */
#include "module_4ha.dtsi" /* Tele CAMERA */

/ {
	fragment@camera {
		target-path = "/";
		__overlay__ {
			#address-cells = <2>;
			#size-cells = <1>;
			interrupt-parent = <&gic>;

			is_flash_s2mf301: is_flash_s2mf301@0 {
				compatible = "samsung,sensor-flash-s2mf301";
				sensor-module = <&is_sensor_hm6>,
						<&is_sensor_4ha>,
						<&is_sensor_2la>;
				id = <0 2 4>;
				led_ch = <1>;
				status = "okay";
			};

			/* UW CAMERA */
			is_sensor_2la: is_sensor_2la@2D {
				pinctrl-names = "pin0", "pin1", "pin2", "release";
				pinctrl-0 = <>;
				pinctrl-1 = <&sensor_mclk1_out>;
				pinctrl-2 = <&sensor_mclk1_fn>;
				pinctrl-3 = <>;

				/* board */
				position = <4>;	/* Rear:0. Front1:1. Tele:2. Front2:3. Ultra Wide:4 */
				id = <4>; /* is_sensor id */
				mclk_ch = <1>;
				sensor_i2c_ch = <4>; /* SENSOR_CONTROL_I2C2 */
				sensor_name = "S5K2LA";

				/* peri */
				af {
					product_name = <100>; /* NOTHING */
				};

				flash {
					product_name = <19>; /* S2MF301 */
				};

				ois {
					product_name = <100>; /* NOTHING */
				};

				power_seq {
					use = <1>;
					gpio_mclk = <&gpg0 2 0x1>;

					scenario_normal_on {
						sensor_scenario = <SENSOR_SCENARIO_NORMAL>;
						gpio_scenario = <GPIO_SCENARIO_ON>;

						/* A start node number should be over the 10 for reserved number 0~9. */
						/* pname = "name"; pin = <act value dealy voltage>; gpio = <gpx# # 0x1>; */
						3 { pname; pin; gpio; };
						5 { pname; pin; gpio; };
						10 { pname = "sen_rst low"; pin = <PIN_OUTPUT 0 0>; gpio = <&gpp0 7 0x1>; };
						20 { pname = "VDDA_2.8_UW_CAM"; pin = <PIN_REGULATOR 1 0>; };
						30 { pname = "VDDD_1.05V_CAM"; pin = <PIN_REGULATOR 1 0>; };
						40 { pname = "VDDIO_1.8V_CAM"; pin = <PIN_REGULATOR 1 0>; share = <SRT_ACQUIRE SHARED_PIN0 1>;};
						50 { pname = "on_i2c"; pin = <PIN_I2C 1 10>; };
						60 { pname = "sen_rst high"; pin = <PIN_OUTPUT 1 300>; gpio = <&gpp0 7 0x1>; };
						70 { pname = "pin"; pin = <PIN_FUNCTION 2 0>; };
						80 { pname = "MCLK"; pin = <PIN_MCLK 1 2000>; };
					};

					scenario_vision_on {
						sensor_scenario = <SENSOR_SCENARIO_VISION>;
						gpio_scenario = <GPIO_SCENARIO_ON>;

						/* A start node number should be over the 10 for reserved number 0~9. */
						/* pname = "name"; pin = <act value dealy voltage>; gpio = <gpx# # 0x1>; */
						3 { pname; pin; gpio; };
						5 { pname; pin; gpio; };
						10 { pname = "sen_rst low"; pin = <PIN_OUTPUT 0 0>; gpio = <&gpp0 7 0x1>; };
						20 { pname = "VDDA_2.8_UW_CAM"; pin = <PIN_REGULATOR 1 0>; };
						30 { pname = "VDDD_1.05V_CAM"; pin = <PIN_REGULATOR 1 0>; };
						40 { pname = "VDDIO_1.8V_CAM"; pin = <PIN_REGULATOR 1 0>; share = <SRT_ACQUIRE SHARED_PIN0 1>;};
						50 { pname = "on_i2c"; pin = <PIN_I2C 1 10>; };
						60 { pname = "sen_rst high"; pin = <PIN_OUTPUT 1 300>; gpio = <&gpp0 7 0x1>; };
						70 { pname = "pin"; pin = <PIN_FUNCTION 2 0>; };
						80 { pname = "MCLK"; pin = <PIN_MCLK 1 2000>; };
					};

					scenario_hw_init_normal_on {
						/* HW INIT (without ois power control) */
						sensor_scenario = <SENSOR_SCENARIO_HW_INIT>;
						gpio_scenario = <GPIO_SCENARIO_ON>;

						10 { pname; pin; gpio; };
						20 { pname; pin; gpio; };
						30 { pname; pin; gpio; };
						40 { pname; pin; gpio; };
						50 { pname; pin; gpio; };
						60 { pname; pin; gpio; };
						70 { pname; pin; gpio; };
						80 { pname; pin; gpio; };
						90 { pname; pin; gpio; };
						100 { pname; pin; gpio; };
						110 { pname; pin; gpio; };
						120 { pname; pin; gpio; };
					};

					scenario_normal_off {
						sensor_scenario = <SENSOR_SCENARIO_NORMAL>;
						gpio_scenario = <GPIO_SCENARIO_OFF>;

						/* A start node number should be over the 10 for reserved number 0~9. */
						/* pname = "name"; pin = <act value dealy voltage>; gpio = <gpx# # 0x1>; */
						10 { pname = "sen_rst low"; pin = <PIN_OUTPUT 0 0>; gpio = <&gpp0 7 0x1>; };
						20 { pname = "off_i2c"; pin = <PIN_I2C 0 0>; };
						30 { pname = "VDDIO_1.8V_CAM"; pin = <PIN_REGULATOR 0 0>; share = <SRT_RELEASE SHARED_PIN0 0>;};
						40 { pname = "VDDD_1.05V_CAM"; pin = <PIN_REGULATOR 0 0>; };
						50 { pname = "VDDA_2.8_UW_CAM"; pin = <PIN_REGULATOR 0 0>; };
						60 { pname = "MCLK"; pin = <PIN_MCLK 0 0>; };
						70 { pname = "pin"; pin = <PIN_OUTPUT 0 0>; };
						140 { pname; pin; gpio; };
						145 { pname; pin; gpio; };
					};

					scenario_vision_off {
						sensor_scenario = <SENSOR_SCENARIO_VISION>;
						gpio_scenario = <GPIO_SCENARIO_OFF>;

						/* A start node number should be over the 10 for reserved number 0~9. */
						/* pname = "name"; pin = <act value dealy voltage>; gpio = <gpx# # 0x1>; */
						10 { pname = "sen_rst low"; pin = <PIN_OUTPUT 0 0>; gpio = <&gpp0 7 0x1>; };
						20 { pname = "off_i2c"; pin = <PIN_I2C 0 0>; };
						30 { pname = "VDDIO_1.8V_CAM"; pin = <PIN_REGULATOR 0 0>; share = <SRT_RELEASE SHARED_PIN0 0>;};
						40 { pname = "VDDD_1.05V_CAM"; pin = <PIN_REGULATOR 0 0>; };
						50 { pname = "VDDA_2.8_UW_CAM"; pin = <PIN_REGULATOR 0 0>; };
						60 { pname = "MCLK"; pin = <PIN_MCLK 0 0>; };
						70 { pname = "pin"; pin = <PIN_OUTPUT 0 0>; };
						140 { pname; pin; gpio; };
						145 { pname; pin; gpio; };
					};
				};
			};

			/* REAR CAMERA */
			is_sensor_hm6: is_sensor_hm6@2D {
				pinctrl-names = "pin0", "pin1", "pin2", "release";
				pinctrl-0 = <>;
				pinctrl-1 = <&sensor_mclk0_out>;
				pinctrl-2 = <&sensor_mclk0_fn>;
				pinctrl-3 = <>;

				/* board */
				position = <0>; /* Rear:0. Front:1, Tele:2 */
				id = <0>; /* is_sensor id */
				mclk_ch = <0>;
				mclk_freq = <19200>;
				sensor_i2c_ch = <0>; /* SENSOR_CONTROL_I2C0 */

				/* peri */
				af {
					product_name = <18>; /* ACTUATOR_NAME_AK737X */
					i2c_ch = <1>; /* SENSOR_CONTROL_I2C1 */
				};

				flash {
					product_name = <19>; /* S2MF301 */
				};

				ois {
					product_name = <100>; /* NOTHING */
				};

				eeprom {
					product_name = <100>;
				};

				power_seq {
					gpio_mclk = <&gpg0 1 0x1>;
					scenario_normal_on {
						10 { pname = "sen_rst low";     pin = <PIN_OUTPUT 0 1000>; gpio = <&gpg2 1 0x1>; };
						20 { pname = "VDDIO_1.8V_CAM"; pin = <PIN_REGULATOR 1 0>; share = <SRT_ACQUIRE SHARED_PIN0 1>; };
						30 { pname; pin; gpio; };
						40 { pname = "RCAM1_AVDD_1P8";  pin = <PIN_OUTPUT 1 1000>; gpio = <&gpp3 6 0x1>; };  /* AFVDD, DVDD  OISVDD*/
						50 { pname = "pin";             pin = <PIN_FUNCTION 2 0>; };
						60 { pname = "MCLK";            pin = <PIN_MCLK 1 0>; gpio = <&gpg0 1 0x1>; };
						70 { pname = "delay";           pin = <PIN_NONE 0 13000>; };
						80 { pname = "sen_rst high";    pin = <PIN_OUTPUT 1 50>; gpio = <&gpg2 1 0x1>; };
						85 { pname = "on_i2c";          pin = <PIN_I2C 1 0>; };
						90 { pname; pin; gpio; };
						100 { pname = "delay";          pin = <PIN_NONE 0 0>; };
					};

					scenario_vision_on {
						sensor_scenario = <SENSOR_SCENARIO_VISION>;
						gpio_scenario = <GPIO_SCENARIO_ON>;
						10 { pname = "sen_rst low";     pin = <PIN_OUTPUT 0 1000>; gpio = <&gpg2 1 0x1>; };
						20 { pname = "VDDIO_1.8V_CAM"; pin = <PIN_REGULATOR 1 0>; share = <SRT_ACQUIRE SHARED_PIN0 1>; };
						30 { pname; pin; gpio; };
						40 { pname = "RCAM1_AVDD_1P8";  pin = <PIN_OUTPUT 1 1000>; gpio = <&gpp3 6 0x1>; };  /* AFVDD, DVDD  OISVDD*/
						50 { pname = "pin";             pin = <PIN_FUNCTION 2 0>; };
						60 { pname = "MCLK";            pin = <PIN_MCLK 1 0>; gpio = <&gpg0 1 0x1>; };
						70 { pname = "delay";           pin = <PIN_NONE 0 1000>; };
						80 { pname = "on_i2c";          pin = <PIN_I2C 1 0>; };
						85 { pname; pin; gpio; };
						90 { pname = "sen_rst high";    pin = <PIN_OUTPUT 1 50>; gpio = <&gpg2 1 0x1>; };
						100 { pname = "delay";          pin = <PIN_NONE 0 11000>; };
					};

					scenario_hw_init_normal_on {
						/* HW INIT (without ois power control) */
						sensor_scenario = <SENSOR_SCENARIO_HW_INIT>;
						gpio_scenario = <GPIO_SCENARIO_ON>;

						10 { pname; pin; gpio; };
						20 { pname; pin; gpio; };
						30 { pname; pin; gpio; };
						40 { pname; pin; gpio; };
						50 { pname; pin; gpio; };
						60 { pname; pin; gpio; };
						70 { pname; pin; gpio; };
						80 { pname; pin; gpio; };
						90 { pname; pin; gpio; };
						100 { pname; pin; gpio; };
						110 { pname; pin; gpio; };
						120 { pname; pin; gpio; };
					};

					scenario_normal_off {
						10 { pname = "sen_rst low"; pin = <PIN_OUTPUT 0 500>; gpio = <&gpg2 1 0x1>; };
						15 { pname; pin; gpio; };
						20 { pname = "VDDIO_1.8V_CAM"; pin = <PIN_REGULATOR 0 500>; share = <SRT_ACQUIRE SHARED_PIN0 0>; };
						30 { pname; pin; gpio; };
						40 { pname = "RCAM1_AVDD_1P8";  pin = <PIN_OUTPUT 0 0>; gpio = <&gpp3 6 0x1>; };  /* AFVDD, DVDD  OISVDD*/
						50 { pname = "MCLK";            pin = <PIN_MCLK 0 500>; gpio = <&gpg0 1 0x1>; };
						60 { pname = "pin"; pin = <PIN_FUNCTION 0 0>; };
						70 { pname = "pin"; pin = <PIN_FUNCTION 1 0>; };
						80 { pname = "pin"; pin = <PIN_FUNCTION 0 0>; };
						90 { pname = "off_i2c"; pin = <PIN_I2C 0 0>; };
						100 { pname = "delay"; pin = <PIN_NONE 0 10000>; };
					};

					scenario_vision_off {
						sensor_scenario = <SENSOR_SCENARIO_VISION>;
						gpio_scenario = <GPIO_SCENARIO_OFF>;
						10 { pname = "sen_rst low"; pin = <PIN_OUTPUT 0 500>; gpio = <&gpg2 1 0x1>; };
						15 { pname; pin; gpio; };
						20 { pname = "VDDIO_1.8V_CAM"; pin = <PIN_REGULATOR 0 500>; share = <SRT_ACQUIRE SHARED_PIN0 0>; };
						30 { pname; pin; gpio; };
						40 { pname = "RCAM1_AVDD_1P8";  pin = <PIN_OUTPUT 0 0>; gpio = <&gpp3 6 0x1>; };  /* AFVDD, DVDD  OISVDD*/
						50 { pname = "MCLK";            pin = <PIN_MCLK 0 500>; gpio = <&gpg0 1 0x1>; };
						60 { pname = "pin"; pin = <PIN_FUNCTION 0 0>; };
						70 { pname = "pin"; pin = <PIN_FUNCTION 1 0>; };
						80 { pname = "pin"; pin = <PIN_FUNCTION 0 0>; };
						90 { pname = "off_i2c"; pin = <PIN_I2C 0 0>; };
						100 { pname = "delay"; pin = <PIN_NONE 0 10000>; };
					};

					scenario_ois_factory_on {
						sensor_scenario = <SENSOR_SCENARIO_OIS_FACTORY>;
						gpio_scenario = <GPIO_SCENARIO_ON>;

						/* A start node number should be over the 10 for reserved number 0~9. */
						/* pname = "name"; pin = <act value delay voltage>; gpio = <gpx# # 0x1>; */
						20 { pname; pin; gpio; };
						30 { pname; pin; gpio; };
						40 { pname; pin; gpio; };
						70 { pname; pin; gpio; };
						80 { pname; pin; gpio; };
						85 { pname; pin; gpio; };
					};

					scenario_ois_factory_off {
						sensor_scenario = <SENSOR_SCENARIO_OIS_FACTORY>;
						gpio_scenario = <GPIO_SCENARIO_OFF>;

						/* A start node number should be over the 10 for reserved number 0~9. */
						/* pname = "name"; pin = <act value delay voltage>; gpio = <gpx# # 0x1>; */
						15 { pname; pin; gpio; };
						20 { pname; pin; gpio; };
						30 { pname; pin; gpio; };
						40 { pname; pin; gpio; };
						90 { pname; pin; gpio; };
					};
					scenario_read_rom_on {
						20 { pname = "VDDIO_1.8V_CAM"; pin = <PIN_REGULATOR 1 0>; share = <SRT_ACQUIRE SHARED_PIN0 1>; };
					};

					scenario_read_rom_off {
						20 { pname = "VDDIO_1.8V_CAM"; pin = <PIN_REGULATOR 0 500>; share = <SRT_ACQUIRE SHARED_PIN0 0>; };
					};
				};
			};

			/* FRONT CAMERA */
			is_sensor_3lu: is_sensor_3lu@10 {
				pinctrl-names = "pin0", "pin1", "pin2", "release";
				pinctrl-0 = <>;
				pinctrl-1 = <&sensor_mclk3_out>;
				pinctrl-2 = <&sensor_mclk3_fn>;
				pinctrl-3 = <>;

				/* board */
				position = <1>;	/* Rear:0. Front:1. Rear_sub:2. Secure:3. */
				id = <1>; /* is_sensor id */
				mclk_ch = <3>;
				sensor_i2c_ch = <1>; /* SENSOR_CONTROL_I2C1 */

				/* peri */
				af {
					product_name = <18>; /* ACTUATOR_NAME_AK737X */
					i2c_ch = <5>; /* SENSOR_CONTROL_I2C5 */
				};

				flash {
					product_name = <100>; /* S2MF301 */
				};

				ois {
					product_name = <100>; /* NOTHING */
				};

				vc_extra {
					/* ex) statX = <stat_type, sensor_mode, max_width, max_height, max_element> */
					stat0 = </* not avaliable */>;	/* VC_BUF_DATA_TYPE_SENSOR_STAT1 */
					stat1 = </* not avaliable */>;	/* VC_BUF_DATA_TYPE_GENERAL_STAT1 */
					stat2 = </* not avaliable */>;	/* VC_BUF_DATA_TYPE_SENSOR_STAT2 */
					stat3 = </* not avaliable */>;	/* VC_BUF_DATA_TYPE_GENERAL_STAT2 */
				};

				modes {
				/* common = <width, height, fps, settle, mode, lane, speed, interleave, lrte, pd_mode> */
				/* vcX = <map, hwformat, width, height, hwformat, type, width, height> */
					mode0_secure { /* A-17 */
						common = <4000 3000 60 0 0 CSI_DATA_LANES_3 1862 CSI_MODE_VC_DT
									 LRTE_ENABLE PD_NONE EX_PDSTAT_OFF>;
						vc0 = </* in */ 0 HW_FORMAT_RAW10 DATA_IMG 4000 3000
								/* out */ HW_FORMAT_RAW10 VC_NOTHING 2000 1500>;
						vc1 = </* in */ 2 HW_FORMAT_UNKNOWN DATA_NONE 0 0
								/* out */ HW_FORMAT_UNKNOWN VC_NOTHING 0 0>;
						vc2 = </* in */ 1 HW_FORMAT_RAW10_POTF DATA_HPD 4000 750
								/* out */ HW_FORMAT_RAW10 VC_NOTHING 0 0>;
						vc3 = </* in */ 3 HW_FORMAT_UNKNOWN DATA_NONE 0 0
								/* out */ HW_FORMAT_UNKNOWN VC_NOTHING 0 0>;
						option { votf = <0>; hw_bns = <HW_BNS_2_0>; };
					};
				};

				power_seq {
					use = <1>;
					gpio_mclk = <&gpg1 1 0x1>;

					scenario_normal_on {
						sensor_scenario = <SENSOR_SCENARIO_NORMAL>;
						gpio_scenario = <GPIO_SCENARIO_ON>;

						/* A start node number should be over the 10 for reserved number 0~9. */
						/* pname = "name"; pin = <act value dealy voltage>; gpio = <gpx# # 0x1>; */
						5 { pname; pin; gpio; share; };
						10 { pname = "sen_rst low"; pin = <PIN_OUTPUT 0 0>; gpio = <&gpp0 6 0x1>; share; };
						15 { pname = "VDDIO_1.8V_CAM"; pin = <PIN_REGULATOR 1 1>; share = <SRT_ACQUIRE SHARED_PIN0 1>; };
						20 { pname = "VDDA_2.2V_VT"; pin = <PIN_REGULATOR 1 0>; };
						30 { pname = "VDDD_0.92V_VT"; pin = <PIN_REGULATOR 1 1500>; };
						40 { pname = "VDDAF_2.8_VT_CAM"; pin = <PIN_REGULATOR 1 0>; share = <SRT_ACQUIRE SHARED_PIN1 1>; };
						50 { pname = "sen_rst high"; pin = <PIN_OUTPUT 1 6000>; gpio = <&gpp0 6 0x1>; };
						60 { pname = "on_i2c"; pin = <PIN_I2C 1 10>; gpio;};
						70 { pname = "pin"; pin = <PIN_FUNCTION 2 0>; };
						80 { pname = "MCLK"; pin = <PIN_MCLK 1 11500>; };
					};

					scenario_vision_on {
						sensor_scenario = <SENSOR_SCENARIO_VISION>;
						gpio_scenario = <GPIO_SCENARIO_ON>;

						/* A start node number should be over the 10 for reserved number 0~9. */
						/* pname = "name"; pin = <act value dealy voltage>; gpio = <gpx# # 0x1>; */
						5 { pname; pin; gpio; share; };
						10 { pname = "sen_rst low"; pin = <PIN_OUTPUT 0 0>; gpio = <&gpp0 6 0x1>; share; };
						15 { pname = "VDDIO_1.8V_CAM"; pin = <PIN_REGULATOR 1 1>; share = <SRT_ACQUIRE SHARED_PIN0 1>; };
						20 { pname = "VDDA_2.2V_VT"; pin = <PIN_REGULATOR 1 0>; };
						30 { pname = "VDDD_0.92V_VT"; pin = <PIN_REGULATOR 1 1500>; share; };
						40 { pname = "VDDAF_2.8_VT_CAM"; pin = <PIN_REGULATOR 1 0>; share = <SRT_ACQUIRE SHARED_PIN1 1>; };
						50 { pname = "on_i2c"; pin = <PIN_I2C 1 10>; };
						60 { pname = "sen_rst high"; pin = <PIN_OUTPUT 1 0>; gpio = <&gpp0 6 0x1>; };
						70 { pname = "pin"; pin = <PIN_FUNCTION 2 0>; };
						80 { pname = "MCLK"; pin = <PIN_MCLK 1 11500>; };
					};

					scenario_hw_init_normal_on {
						/* HW INIT (without ois power control) */
						sensor_scenario = <SENSOR_SCENARIO_HW_INIT>;
						gpio_scenario = <GPIO_SCENARIO_ON>;

						5 { pname; pin; gpio; share; };
						10 { pname = "sen_rst low"; pin = <PIN_OUTPUT 0 0>; gpio = <&gpp0 6 0x1>; share; };
						15 { pname = "VDDIO_1.8V_CAM"; pin = <PIN_REGULATOR 1 1>; share = <SRT_ACQUIRE SHARED_PIN0 1>; };
						20 { pname = "VDDA_2.2V_VT"; pin = <PIN_REGULATOR 1 0>; };
						30 { pname = "VDDD_0.92V_VT"; pin = <PIN_REGULATOR 1 1500>; };
						40 { pname = "VDDAF_2.8_VT_CAM"; pin = <PIN_REGULATOR 1 0>; share = <SRT_ACQUIRE SHARED_PIN1 1>; };
						50 { pname = "on_i2c"; pin = <PIN_I2C 1 10>; };
						60 { pname = "sen_rst high"; pin = <PIN_OUTPUT 1 0>; gpio = <&gpp0 6 0x1>; };
						70 { pname = "pin"; pin = <PIN_FUNCTION 2 0>; };
						80 { pname = "MCLK"; pin = <PIN_MCLK 1 11500>; };
					};

					scenario_normal_off {
						sensor_scenario = <SENSOR_SCENARIO_NORMAL>;
						gpio_scenario = <GPIO_SCENARIO_OFF>;

						/* A start node number should be over the 10 for reserved number 0~9. */
						/* pname = "name"; pin = <act value dealy voltage>; gpio = <gpx# # 0x1>; */
						10 { pname = "MCLK"; pin = <PIN_MCLK 0 1>; };
						20 { pname = "pin"; pin = <PIN_FUNCTION 0 0>; };
						30 { pname = "pin"; pin = <PIN_FUNCTION 1 0>; };
						40 { pname = "pin"; pin = <PIN_FUNCTION 0 0>; };
						50 { pname = "rst_low"; pin = <PIN_OUTPUT 0 0>; gpio = <&gpp0 6 0x1>; };
						60 { pname = "off_i2c"; pin = <PIN_I2C 0 0>; };
						70 { pname = "VDDAF_2.8_VT_CAM"; pin = <PIN_REGULATOR 0 0>; share = <SRT_RELEASE SHARED_PIN1 0>; };
						80 { pname = "VDDD_0.92V_VT"; pin = <PIN_REGULATOR 0 0>; };
						90 { pname = "VDDA_2.2V_VT"; pin = <PIN_REGULATOR 0 0>; };
						100 { pname = "VDDIO_1.8V_CAM"; pin = <PIN_REGULATOR 0 0>; share = <SRT_RELEASE SHARED_PIN0 0>; };
						110 { pname; pin; gpio; share; };
						120 { pname; pin; gpio; share; };
					};

					scenario_vision_off {
						sensor_scenario = <SENSOR_SCENARIO_VISION>;
						gpio_scenario = <GPIO_SCENARIO_OFF>;

						/* A start node number should be over the 10 for reserved number 0~9. */
						/* pname = "name"; pin = <act value dealy voltage>; gpio = <gpx# # 0x1>; */
						10 { pname = "MCLK"; pin = <PIN_MCLK 0 1>; };
						20 { pname = "pin"; pin = <PIN_FUNCTION 0 0>; };
						30 { pname = "pin"; pin = <PIN_FUNCTION 1 0>; };
						40 { pname = "pin"; pin = <PIN_FUNCTION 0 0>; };
						50 { pname = "rst_low"; pin = <PIN_OUTPUT 0 0>; gpio = <&gpp0 6 0x1>; };
