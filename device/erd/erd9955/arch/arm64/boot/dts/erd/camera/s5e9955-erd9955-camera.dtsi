	is-actuator@35 { /* uwide AF */
		compatible = "samsung,exynos-is-actuator-ak737x";
		reg = <0x35>;
		sensor-module = <&is_sensor_imx564>;
		id = <4>; /* matching is_sensor id */
		/* DW9825A */
		vendor_product_id = <0xF3 0x0E>;
		vendor_first_pos = <960>;  /* 12bits */
