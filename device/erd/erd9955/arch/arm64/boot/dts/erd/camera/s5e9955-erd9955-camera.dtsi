#include <dt-bindings/camera/exynos_is_dt.h>

#include "module_hp2.dtsi"
#include "module_imx754.dtsi"
#include "module_imx754_mirrorflip.dtsi"
#include "module_imx564.dtsi"
#include "module_3lu.dtsi"

/ {
	fragment@camera {
		target-path = "/";
		__overlay__ {
			#address-cells = <2>;
			#size-cells = <1>;
			interrupt-parent = <&gic>;

			is_flash_gpio: is-flash-gpio@0 {
				compatible = "samsung,sensor-flash-s2mpb02";
				sensor-module = <&is_sensor_hp2>,
						<&is_sensor_imx564>,
						<&is_sensor_imx754>,
						<&is_sensor_imx754_mirrorflip>;
				id = <0 4 2 3>;
				status = "okay";

				torch-gpio = <&gpc3 1 0x1>;
			};

			is_sensor_hp2: is_sensor_hp2@20 {
				modes {
						/* common = <width, height, fps, settle, mode, lane, speed, interleave, lrte, pd_mode> */
						/* vcX = <map, hwformat, width, height, hwformat, type, width, height> */
					mode10 { /* B-01 SENSOR_HP2_8160x6120_30FPS_4SUM */
						common = <8160 6120 30 0 10 CSI_DATA_LANES_3 3712 CSI_MODE_VC_DT LRTE_DISABLE PD_MOD3>;
						vc0 = </* in */ 0 HW_FORMAT_RAW10 DATA_IMG 8160 6120		/* out */ HW_FORMAT_RAW10 VC_NOTHING 8160 6120>;
						vc1 = </* in */ 2 HW_FORMAT_UNKNOWN DATA_NONE 0 0			/* out */ HW_FORMAT_UNKNOWN VC_NOTHING 0 0>;
						vc2 = </* in */ 1 HW_FORMAT_RAW10_POTF DATA_HPD 4080 764	/* out */ HW_FORMAT_RAW10_POTF_PACK VC_TAILPDAF 4080 764>;
						vc3 = </* in */ 3 HW_FORMAT_UNKNOWN DATA_NONE 0 0			/* out */ HW_FORMAT_UNKNOWN VC_NOTHING 0 0>;
						option { votf = <0>; special_mode = <IS_SPECIAL_MODE_REMOSAIC>; hw_bns = <HW_BNS_2_0>; };
					};
				};

				power_seq {
					scenario_normal_on {
						10 {
							pname = "sen_rst low";
							pin = <PIN_OUTPUT 0 0>;
							gpio = <&gpp5 0 0x1>;
						};
						110 {
							pname = "sen_rst high";
							pin = <PIN_OUTPUT 1 7870>;
							gpio = <&gpp5 0 0x1>;
						};
					};

					scenario_vision_on {
						10 {
							pname = "sen_rst low";
							pin = <PIN_OUTPUT 0 0>;
							gpio = <&gpp5 0 0x1>;
						};
						110 {
							pname = "sen_rst high";
							pin = <PIN_OUTPUT 1 7870>;
							gpio = <&gpp5 0 0x1>;
						};
					};

					scenario_normal_off {
						50 {
							pname = "(retention) sen_rst low";
							pin = <PIN_OUTPUT 0 10>;
							gpio = <&gpp5 0 0x1>;
						};
					};

					scenario_vision_off {
						50 {
							pname = "(retention) sen_rst low";
							pin = <PIN_OUTPUT 0 10>;
							gpio = <&gpp5 0 0x1>;
						};
					};

					scenario_retention_on {
						50 {
							pname = "(retention) sen_rst low";
							pin = <PIN_OUTPUT 0 10>;
							gpio = <&gpp5 0 0x1>;
						};
					};
				};
			};

			is_sensor_imx564: is_sensor_imx564@1A {
				power_seq {
					scenario_normal_on {
						10 {
							pname = "sen_rst low";
							pin = <PIN_OUTPUT 0 0>;
							gpio = <&gpp5 1 0x1>;
						};
						80 {
							pname = "sen_rst high";
							pin = <PIN_OUTPUT 1 0>;
							gpio = <&gpp5 1 0x1>;
						};
					};

					scenario_vision_on {
						10 {
							pname = "sen_rst low";
							pin = <PIN_OUTPUT 0 0>;
							gpio = <&gpp5 1 0x1>;
						};
						80 {
							pname = "sen_rst high";
							pin = <PIN_OUTPUT 1 0>;
							gpio = <&gpp5 1 0x1>;
						};
					};

					scenario_normal_off {
						10 {
							pname = "sen_rst low";
							pin = <PIN_OUTPUT 0 0>;
							gpio = <&gpp5 1 0x1>;
						};
					};

					scenario_vision_off {
						10 {
							pname = "sen_rst low";
							pin = <PIN_OUTPUT 0 0>;
							gpio = <&gpp5 1 0x1>;
						};
					};
				};
			};

			is_sensor_imx754: is_sensor_imx754@1A {
				power_seq {
					scenario_normal_on {
						80 {
							pname = "sen_rst high";
							pin = <PIN_OUTPUT 1 2400>;
							gpio = <&gpp5 2 0x1>;
						};
					};

					scenario_vision_on {
						80 {
							pname = "sen_rst high";
							pin = <PIN_OUTPUT 1 2400>;
							gpio = <&gpp5 2 0x1>;
						};
					};

					scenario_normal_off {
						10 {
							pname = "sen_rst low";
							pin = <PIN_OUTPUT 0 1800>;
							gpio = <&gpp5 2 0x1>;
						};
					};

					scenario_vision_off {
						10 {
							pname = "sen_rst low";
							pin = <PIN_OUTPUT 0 1800>;
							gpio = <&gpp5 2 0x1>;
						};
					};
				};
			};

			is_sensor_imx754_mirrorflip: is_sensor_imx754_mirrorflip@1A {
				power_seq {
					scenario_normal_on {
						70 {
							pname = "sen_rst high";
							pin = <PIN_OUTPUT 1 2400>;
							gpio = <&gpp5 3 0x1>;
						};
					};

					scenario_vision_on {
						70 {
							pname = "sen_rst high";
							pin = <PIN_OUTPUT 1 2400>;
							gpio = <&gpp5 3 0x1>;
						};
					};

					scenario_normal_off {
						10 {
							pname = "sen_rst_low";
							pin = <PIN_OUTPUT 0 1800>;
							gpio = <&gpp5 3 0x1>;
						};
					};

					scenario_vision_off {
						10 {
							pname = "sen_rst_low";
							pin = <PIN_OUTPUT 0 1800>;
							gpio = <&gpp5 3 0x1>;
						};
					};
				};
			};

			is_sensor_3lu: is_sensor_3lu@10 {
				sensor_i2c_ch = <6>; /* SENSOR_CONTROL_I2C6 */

				/* peri*/
				af {
					i2c_ch = <7>; /* SENSOR_CONTROL_I2C7 */
				};

				power_seq {
					scenario_normal_on {
						60 {
							pname = "rst_high";
							pin = <PIN_OUTPUT 1 0>;
							gpio = <&gpc3 0 0x1>;
						};
					};

					scenario_vision_on {
						60 {
							pname = "rst_high";
							pin = <PIN_OUTPUT 1 0>;
							gpio = <&gpc3 0 0x1>;
						};
					};

					scenario_normal_off {
						50 {
							pname = "rst_low";
							pin = <PIN_OUTPUT 0 0>;
							gpio = <&gpc3 0 0x1>;
						};
					};

					scenario_vision_off {
						50 {
							pname = "rst_low";
							pin = <PIN_OUTPUT 0 0>;
							gpio = <&gpc3 0 0x1>;
						};
					};
				};
			};

			/* REAR CAMERA */
			is_sensor0: is_sensor0@23880000 {
				reg = <0x0 0x23890000 0x1000>, /* MIPI-CSI1 */
					<0x0 0x238F0000 0x4000>, /* PHY: M0S4S4S4S4S4S4_S22 */
					<0x0 0x239D022C 0x10>, /* CSIS_FRO */
					<0x0 0x239D02A8 0x4>; /* EMB_LC MUX */
				scenario = <SENSOR_SCENARIO_NORMAL>; /* Normal, Vision, OIS etc */

				id = <0>;
				csi_ch = <1>;
				status = "okay";
				use_cphy = <1>;

				interrupts = <0 INTREQ__CSIS1 IRQ_TYPE_LEVEL_HIGH>; /* MIPI-CSI1 */
				phys = <&mipi_dcphy_csis1_m0s4s4s4s4s4s4_s22>;
				phy_setfile = <&dcphy_setfile_cphy>;
			};

			/* FRONT CAMERA */
			is_sensor1: is_sensor1@23890000 {
				reg = <0x0 0x23880000 0x1000>, /* MIPI-CSI0 */
					<0x0 0x238F0000 0x4000>, /* PHY: M0S4S4S4S4S4S4_S22 */
					<0x0 0x239D022C 0x10>, /* CSIS_FRO */
					<0x0 0x239D02A4 0x4>; /* EMB_LC MUX */
				scenario = <SENSOR_SCENARIO_NORMAL>; /* Normal, Vision, OIS etc */
				id = <1>;
				csi_ch = <0>;
				status = "okay";
				use_cphy = <1>;
				scramble = <1>; /* need to sync with sensor setting */

				interrupts = <0 INTREQ__CSIS0 IRQ_TYPE_LEVEL_HIGH>; /* MIPI-CSI0 */
				phys = <&mipi_dcphy_csis0_m0s4s4s4s4s4s4_s22>;
				phy_setfile = <&cphy_3lu_setfile>;
			};

			/* Tele x3 CAMERA */
			is_sensor2: is_sensor2@238A0000 {
				reg = <0x0 0x238A0000 0x1000>, /* MIPI-CSI2 */
					<0x0 0x238F0000 0x4000>, /* PHY: M0S4S4S4S4S4S4_S22 */
					<0x0 0x239D022C 0x10>, /* CSIS_FRO */
					<0x0 0x239D02AC 0x4>; /* EMB_LC MUX */
				scenario = <SENSOR_SCENARIO_NORMAL>; /* Normal, Vision, OIS etc */
				id = <2>;
				csi_ch = <2>;
				status = "okay";
				use_cphy = <1>;

				interrupts = <0 INTREQ__CSIS2 IRQ_TYPE_LEVEL_HIGH>; /* MIPI-CSI2 */
				phys = <&mipi_dcphy_csis2_m0s4s4s4s4s4s4_s22>;
				phy_setfile = <&imx754_dcphy_setfile_cphy>;
			};

			/* Tele x10 CAMERA */
			is_sensor3: is_sensor3@238B0000 {
				reg = <0x0 0x238C0000 0x1000>, /* MIPI-CSI4 */
					<0x0 0x238F0000 0x4000>, /* PHY: M0S4S4S4S4S4S4_S22 */
					<0x0 0x239D022C 0x10>, /* CSIS_FRO */
					<0x0 0x239D02B4 0x4>; /* EMB_LC MUX */
				scenario = <SENSOR_SCENARIO_NORMAL>; /* Normal, Vision, OIS etc */
				id = <3>;
				csi_ch = <4>;
				status = "okay";
				use_cphy = <1>;

				interrupts = <0 INTREQ__CSIS4 IRQ_TYPE_LEVEL_HIGH>; /* MIPI-CSI4 */
				phys = <&mipi_dcphy_csis4_m0s4s4s4s4s4s4_s22>;
				phy_setfile = <&dcphy_setfile_cphy>;
			};

			/* ULTRA WIDE CAMERA */
			is_sensor4: is_sensor4@238C0000 {
				reg = <0x0 0x238B0000 0x1000>, /* MIPI-CSI3 */
					<0x0 0x238F0000 0x4000>, /* PHY: M0S4S4S4S4S4S4_S22 */
					<0x0 0x239D022C 0x10>, /* CSIS_FRO */
					<0x0 0x239D02B0 0x4>; /* EMB_LC MUX */
				scenario = <SENSOR_SCENARIO_NORMAL>; /* Normal, Vision, OIS etc */
				id = <4>;
				csi_ch = <3>;
				status = "okay";
				use_cphy = <1>;

				interrupts = <0 INTREQ__CSIS3 IRQ_TYPE_LEVEL_HIGH>; /* MIPI-CSI3 */
				phys = <&mipi_dcphy_csis3_m0s4s4s4s4s4s4_s22>;
				phy_setfile = <&dcphy_setfile_cphy>;
			};

			is_sensor5: is_sensor5@238D0000 {
				scenario = <SENSOR_SCENARIO_NORMAL>; /* Normal, Vision, OIS etc */
				id = <1>;
				csi_ch = <1>;
				status = "disabled";
			};
		}; /* end of __overlay__ */
	}; /* end of fragment */
}; /* end of root */

/***** reference symbol node *****/
&pinctrl_7 {
	/* REAR WIDE */
	hsi2c6_bus_in: hsi2c6-bus-in {
		samsung,pins = "gpp3-0", "gpp3-1";
		samsung,pin-function = <0>;
		samsung,pin-pud = <0>;
		samsung,pin-drv = <0>;
	};

	/* REAR AF*/
	hsi2c13_bus_in: hsi2c13-bus-in {
		samsung,pins = "gpp6-2", "gpp6-3";
		samsung,pin-function = <0>;
		samsung,pin-pud = <0>;
		samsung,pin-drv = <0>;
	};

	/* REAR UW */
	hsi2c3_bus_in: hsi2c3-bus-in {
		samsung,pins = "gpp1-2", "gpp1-3";
		samsung,pin-function = <0>;
		samsung,pin-pud = <0>;
		samsung,pin-drv = <0>;
	};

	/* REAR UW AF*/
	hsi2c2_bus_in: hsi2c2-bus-in {
		samsung,pins = "gpp1-0", "gpp1-1";
		samsung,pin-function = <0>;
		samsung,pin-pud = <0>;
		samsung,pin-drv = <0>;
	};

	/* REAR TELE 3x */
	hsi2c4_bus_in: hsi2c4-bus-in {
		samsung,pins = "gpp2-0", "gpp2-1";
		samsung,pin-function = <0>;
		samsung,pin-pud = <0>;
		samsung,pin-drv = <0>;
	};

	/* Tele 3x AF */
	hsi2c5_bus_in: hsi2c5-bus-in {
		samsung,pins = "gpp2-2", "gpp2-3";
		samsung,pin-function = <0>;
		samsung,pin-pud = <0>;
		samsung,pin-drv = <0>;
	};

	/* Tele 10x */
	hsi2c7_bus_in: hsi2c7-bus-in {
		samsung,pins = "gpp3-2", "gpp3-3";
		samsung,pin-function = <0>;
		samsung,pin-pud = <0>;
		samsung,pin-drv = <0>;
	};

	/* Tele 10x AF */
	hsi2c12_bus_in: hsi2c12-bus-in {
		samsung,pins = "gpp6-0", "gpp6-1";
		samsung,pin-function = <0>;
		samsung,pin-pud = <0>;
		samsung,pin-drv = <0>;
	};

	/* FRONT */
	hsi2c23_bus_in: hsi2c23_bus_in-bus-in {
		samsung,pins = "gpp11-2", "gpp11-3";
		samsung,pin-function = <0>;
		samsung,pin-pud = <0>;
		samsung,pin-drv = <0>;
	};

	/* FRONT AF*/
	hsi2c22_bus_in: hsi2c22-bus-in {
		samsung,pins = "gpp11-0", "gpp11-1";
		samsung,pin-function = <0>;
		samsung,pin-pud = <0>;
		samsung,pin-drv = <0>;
	};
};

&hsi2c_6 {
	#address-cells = <1>;
	#size-cells = <0>;
	gpios = <&gpp3 0 0 &gpp3 1 0>;
	status = "okay";
	clock-frequency = <1000000>;
	samsung,reset-before-trans;
	samsung,fast-plus-mode;

	pinctrl-names = "default","on_i2c","off_i2c";
	pinctrl-0 = <&hsi2c6_bus_in>;
	pinctrl-1 = <&hsi2c6_bus>;
	pinctrl-2 = <&hsi2c6_bus_in>;

	is-hp2@10 {
		compatible = "samsung,exynos-is-cis-hp2";
		sensor-module = <&is_sensor_hp2>;
		reg = <0x10>; /* 1 bit right shift */
		id = <0>; /* matching is_sensor id */
		setfile = "setA";
		rev_reg = <0x0002 2 0xB000>;
	};
};

&hsi2c_13 {
	#address-cells = <1>;
	#size-cells = <0>;
	gpios = <&gpp6 2 0 &gpp6 3 0>;
	status = "okay";
	clock-frequency = <1000000>;
	samsung,reset-before-trans;
	samsung,fast-plus-mode;

	pinctrl-names = "default","on_i2c","off_i2c";
	pinctrl-0 = <&hsi2c13_bus_in>;
	pinctrl-1 = <&hsi2c13_bus>;
	pinctrl-2 = <&hsi2c13_bus_in>;

	is-actuator@C { /* wide AF */
		compatible = "samsung,exynos-is-actuator-ak737x";
		sensor-module = <&is_sensor_hp2>;
		reg = <0x0C>;
		id = <0>; /* matching is_sensor id */
		/* AK7314: addr 0xF3, data 0x19 */
		vendor_product_id = <0xF3 0x13 0xF3 0x19>;
		vendor_first_pos = <2000>;  /* 12bits */
		vendor_first_delay = <2000>;
		vendor_soft_landing_seqid = <3>;
		vendor_soft_landing_list = <640 30 64 30>; /* 1st_pos delay 2nd_pos delay */
		vendor_use_sleep_mode;
		vendor_use_standby_mode; /* AK7314 with vendor_use_sleep_mode */
		/* delay settings for AK7314 */
		vendor_sleep_to_standby_delay = <2200>;
		vendor_active_to_standby_delay = <300>;
	};
};

&hsi2c_3 {
	#address-cells = <1>;
	#size-cells = <0>;
	gpios = <&gpp1 2 0 &gpp1 3 0>;
	status = "okay";
	clock-frequency = <1000000>;
	samsung,reset-before-trans;
	samsung,fast-plus-mode;

	pinctrl-names = "default","on_i2c","off_i2c";
	pinctrl-0 = <&hsi2c3_bus_in>;
	pinctrl-1 = <&hsi2c3_bus>;
	pinctrl-2 = <&hsi2c3_bus_in>;

	is-imx564@1A { /* ultrawide */
		compatible = "samsung,exynos-is-cis-imx564";
		sensor-module = <&is_sensor_imx564>;
		reg = <0x1A>; /* 1 bit right shift */
		id = <4>; /* matching is_sensor id */
		setfile = "setA";
		status = "okay";
		rev_reg = <0x36F2 2 0x564D>;
		vendor_use_adaptive_mipi;
	};
};

&hsi2c_2 {
	#address-cells = <1>;
	#size-cells = <0>;
