			3  = <0x000C 0 32 0x82090004 IDX_FIX_VAL 0>; /* SC_ANA_CON1 */
			4  = <0x0010 0 32 0x00004000 IDX_FIX_VAL 0>; /* SC_ANA_CON2 */
		};

		lane {
			0  = <0x0000 0 32 0x3C000000 IDX_FIX_VAL 3>; /* CSD_GNR_CON0 */
			1  = <0x000C 0 32 0x82090007 IDX_FIX_VAL 4>; /* CSD/SD_ANA_CON1 */
			2  = <0x0010 0 32 0x00002000 IDX_FIX_VAL 4>; /* SD_ANA_CON2 */
			3  = <0x0010 0 32 0x09004000 IDX_FIX_VAL 3>; /* CSD_ANA_CON2 */
