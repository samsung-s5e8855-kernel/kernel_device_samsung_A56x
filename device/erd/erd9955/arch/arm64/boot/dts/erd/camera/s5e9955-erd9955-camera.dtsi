			10 = <0x0070 0 32 0x00000030 IDX_FIX_VAL 3>; /* CSD_TEST_CON0 */
		};

		eq {
			0 {
				0  = <0x0008 0 32 0xECD2103B IDX_FIX_VAL 3>; /* CSD_ANA_CON0 */

