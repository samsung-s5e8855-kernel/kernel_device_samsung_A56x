/ {
	/* power-scenario mode - app boost */
	set1_2: set@102 {
		tex {
			top-app = <110>;
			foreground = <110>;
		};

		ecs_dynamic { };

		cgroup_boost {
			top-app = <1>;
			foreground = <1>;
		};

		dslt {
			running-sensitivity {
				background = <100>;
				system-background = <100>;
			};
		};
	};
};
